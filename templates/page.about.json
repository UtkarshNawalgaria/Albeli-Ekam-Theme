          "settings": {
            "enable_image": true,
            "image": "shopify:\/\/shop_images\/dl.beatsnoop.com-1659347159FtzR.jpg",
            "image_width": 300,
            "title": "Professional Photoshoot",
            "text": "<p>We will provide you with a professional photoshoot of your showcased products, which will be done in our state-of-the-art in-house content studio. This product portfolio will be used on our website, and can also be used for your own personal dealings.<\/p>",
            "button_label": "",
            "button_link": ""
          }
        },
